#!/bin/bash
set -o errexit
set -o errtrace
set -o pipefail
set -o nounset

problem_list=${AUTOPKGTEST_TMP}/problems.txt
debian/tests/write-problems ${problem_list} test/langtools/ProblemList.txt langtools

debian/tests/jtreg-autopkgtest.sh langtools \
  -exclude:${problem_list} \
  -dir:test/langtools \
    -k:!stress \
    :tier1 :tier2
debian/tests/jtdiff-autopkgtest.sh langtools
